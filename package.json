{
  "name": "node-ios-backend",
  "version": "1.0.0",
  "description": "iOS Back-End Server",
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon ./dist/index.ts",
    "start": "node ./dist/index.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/entity/ormconfig.ts",
    "typeorm:migrate": "yarn typeorm migration:generate -- -n",
    "typeorm:run": "yarn typeorm migration:run",
    "docker:build": "docker-compose up -d --build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "Node.js",
    "Express.js",
    "PostgreSQL",
    "PGADMIN4",
    "TypeScript",
    "TypeORM",
    "SOLID",
    "TDD",
    "Socket.io",
    "Redis"
  ],
  "author": "Jonathan Farber",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/helmet": "^0.0.48",
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
