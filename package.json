{
  "name": "node-ios-backend",
  "version": "1.0.0",
  "description": "iOS Back-End Server",
  "main": "dist/index",
  "scripts": {
    "__comment_1__": "echo '_______________________________ application Scripts ___________________________________'",
    "docs": "./node_modules/.bin/typedoc --out docs --mode modules",
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts'",
    "watch": "tsc -w",
    "dev": "nodemon .",
    "start": "node .",
    "test": "jest --coverage",
    "build": "rm -rf ./dist && tsc",
    "__comment_2__": "echo '_________________________________ TypeORM Scripts _____________________________________'",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/entity/ormconfig.ts",
    "typeorm:migrate": "yarn typeorm migration:generate -n",
    "typeorm:run": "yarn typeorm migration:run",
    "__comment_3__": "echo '_________________________________ Docker Scripts ______________________________________'",
    "docker:dev": "make dev",
    "docker:prod": "make prod",
    "docker:check": "docker ps",
    "docker:teardown": "make teardown",
    "docker:purge": "make purge"
  },
  "keywords": [
    "Node.js",
    "Express.js",
    "PostgreSQL",
    "PGADMIN4",
    "TypeScript",
    "TypeORM",
    "SOLID",
    "DDD",
    "TDD",
    "Socket.io",
    "Redis",
    "Docker"
  ],
  "author": "Jonathan Farber",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FortySix-NTwo/node-ts-reat-api"
  },
  "dependencies": {
    "@sentry/node": "^5.22.3",
    "@types/ioredis": "^4.17.3",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-async-router": "^0.1.15",
    "express-rate-limit": "^5.1.3",
    "express-session": "^1.17.1",
    "helmet": "^4.1.0",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "winston": "^3.3.3",
    "winston-transport-sentry-node": "^0.6.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/express-rate-limit": "^5.1.0",
    "@types/express-session": "^1.17.0",
    "@types/express-winston": "^4.0.0",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.6.2",
    "@types/pg": "^7.14.4",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.2"
  }
}
