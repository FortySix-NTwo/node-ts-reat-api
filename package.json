{
  "name": "node-ios-backend",
  "version": "1.0.0",
  "description": "iOS Back-End Server",
  "main": "dist/index",
  "scripts": {
    "__comment_1__": "echo '_______________________________ application Scripts ___________________________________'",
    "watch": "tsc -w",
    "dev": "nodemon .",
    "start": "node .",
    "test": "jest --coverage",
    "build": "tsc",
    "__comment_2__": "echo '_________________________________ TypeORM Scripts _____________________________________'",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/entity/ormconfig.ts",
    "typeorm:migrate": "yarn typeorm migration:generate -- -n",
    "typeorm:run": "yarn typeorm migration:run",
    "__comment_3__": "echo '_________________________________ Docker Scripts ______________________________________'",
    "docker:build": "docker-compose up -d --build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:check": "docker ps",
    "docker:teardown": "docker container rm -f [postgres:12, dpage/pgadmin4] && docker container prune",
    "docker:purge": "docker system prune"
  },
  "keywords": [
    "Node.js",
    "Express.js",
    "PostgreSQL",
    "PGADMIN4",
    "TypeScript",
    "TypeORM",
    "SOLID",
    "TDD",
    "Socket.io",
    "Redis"
  ],
  "author": "Jonathan Farber",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FortySix-NTwo/node-ts-reat-api"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
